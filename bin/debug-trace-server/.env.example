# Debug Trace Server Integration Test Configuration
# Copy this file to .env and fill in the values

# mega-reth RPC endpoint URL
MEGA_RETH_URL=http://localhost:49945

# debug-trace-server RPC endpoint URL
DEBUG_TRACE_SERVER_URL=http://localhost:18545

# Number of blocks to test (default: 5)
TEST_BLOCK_COUNT=5

# Maximum transactions per block to test (default: 3)
MAX_TX_PER_BLOCK=3

# Request timeout in seconds (default: 120)
REQUEST_TIMEOUT_SECS=120
